
busyboxImage: registry.wuxingdev.cn/third/busybox:1.28.0

ingress:
  enabled: true
  ingressClassName: "external"

  tls:
    secretName: tls-wuxingdev.cn

rancherImage: registry.wuxingdev.cn/google_containers/rancher
rancherImageTag: v2.6.8

resources:
  limits:
    cpu: 1
    memory: 1Gi
  request:
    cpu: 1
    memory: 1Gi

postDelete:
  enabled: true
  image:
    repository: registry.wuxingdev.cn/google_containers/rancher-shell
    tag: v0.1.18

  namespaceList:
    - kube-cattle

